type Knex_migrations @pgTable(name: "knex_migrations") {
  id: Int! @unique
  batch: Int
  migration_time: DateTime
  name: String
}

type Knex_migrations_lock @pgTable(name: "knex_migrations_lock") {
  id: Int! @unique @pgColumn(name: "index")
  is_locked: Int
}

type Users @pgTable(name: "users") {
  id: Int! @unique
  created_at: DateTime!
  email: String @unique
  first_name: String
  last_name: String
  updated_at: DateTime!
  username: String @unique
}